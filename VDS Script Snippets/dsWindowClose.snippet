<?xml version="1.0"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>dsWindow Close</Title>
			<Shortcut />
			<Description>Execute code hitting the OK button; call this snippet to finally close the dialog.</Description>
			<Author />
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Code Language="PowerShell" Delimiter="~">
        <![CDATA[$dsWindow.CloseWindowCommand.Execute($this)]]></Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>